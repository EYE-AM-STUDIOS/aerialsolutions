{
  "name": "aerial-solutions",
  "version": "2.0.0",
  "description": "EDIS - Eye Aerial Imaging Services Dashboard supporting both Aerial and Ground-Based Imaging",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "dev": "python -m http.server 3000",
    "build": "echo 'No build step required for static site'",
    "deploy": "vercel --prod",
    "preview": "vercel",
    "new:client": "node scripts/new-client.js",
    "new:client:demo": "node scripts/new-client.js --id demo-mosaic --name 'Demo Mosaic Client' --email demo@test.com",
    "activate:client": "node scripts/activate-client.js",
    "activate:demo": "node scripts/activate-client.js --id demo-mosaic --deposit true --status active",
    "cloudinary:upload-aerial": "node scripts/cloudinary-upload.js upload --service=aerial",
    "cloudinary:upload-ground": "node scripts/cloudinary-upload.js upload --service=ground",
    "cloudinary:upload-all": "node scripts/cloudinary-upload.js upload-all", 
    "cloudinary:list": "node scripts/cloudinary-upload.js list",
    "cloudinary:delete": "node scripts/cloudinary-upload.js delete",
    "cloudinary:usage": "node scripts/cloudinary-monitor.js",
    "cloudinary:usage:brief": "node scripts/cloudinary-monitor.js brief",
    "cloudinary:test": "node scripts/test-integration.js"
  },
  "keywords": [
    "aerial-imaging",
    "ground-imaging", 
    "drone-services",
    "traditional-photography",
    "orthomosaic",
    "3d-mapping",
    "equipment-documentation",
    "site-survey",
    "florida",
    "edis"
  ],
  "author": "EDIS - Eye Aerial Imaging Services",
  "license": "MIT",
  "devDependencies": {
    "cloudinary": "^1.41.0"
  },
  "dependencies": {
    "cloudinary": "^1.41.0",
    "dotenv": "^16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EYE-AM-STUDIOS/edis.git"
  },
  "homepage": "https://edis-aerial.vercel.app",
  "engines": {
    "node": ">=14.0.0"
  }
}