{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/signin",
      "dest": "/signin.html"
    },
    {
      "src": "/portal",
      "dest": "/portal.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/admin-dashboard.html"
    }
  ]
}