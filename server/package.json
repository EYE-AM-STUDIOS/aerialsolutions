{
  "name": "edis-portal-server",
  "version": "1.0.0",
  "description": "EDIS Portal Server with HoneyBook Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run build:client",
    "build:client": "npm run build --prefix ../",
    "deploy": "npm run build && pm2 restart edis-portal",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "edis",
    "portal",
    "honeybook",
    "imaging",
    "client-management"
  ],
  "author": "EYE AM STUDIOS",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^6.10.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.4",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "cloudinary": "^1.41.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "@types/node": "^20.4.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EYE-AM-STUDIOS/edis-portal.git"
  },
  "bugs": {
    "url": "https://github.com/EYE-AM-STUDIOS/edis-portal/issues"
  },
  "homepage": "https://edis-imaging.com"
}