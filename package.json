{
  "name": "eas-aerial-imaging-solutions",
  "version": "1.0.0",
  "description": "EAS | Aerial Imaging Solutions - Professional drone services with client portal system",
  "main": "index.html",
  "scripts": {
    "dev": "python -m http.server 8000 || php -S localhost:8000 || live-server",
    "build": "echo 'Static site - no build needed'",
    "deploy": "vercel --prod",
    "preview": "vercel",
    "new:client": "node scripts/new-client.js",
    "new:client:demo": "node scripts/new-client.js --id demo-mosaic --name 'Demo Mosaic Client' --email demo@test.com",
    "activate:client": "node scripts/activate-client.js",
    "activate:demo": "node scripts/activate-client.js --id demo-mosaic --deposit true --status active",
    "cloudinary:upload": "node scripts/cloudinary-upload.js upload",
    "cloudinary:upload-all": "node scripts/cloudinary-upload.js upload-all", 
    "cloudinary:list": "node scripts/cloudinary-upload.js list",
    "cloudinary:delete": "node scripts/cloudinary-upload.js delete",
    "cloudinary:usage": "node scripts/cloudinary-monitor.js",
    "cloudinary:usage:brief": "node scripts/cloudinary-monitor.js brief"
  },
  "keywords": [
    "aerial-imaging",
    "drone-services",
    "orthomosaic",
    "3d-mapping",
    "florida",
    "client-portal"
  ],
  "author": "EYE AM STUDIOS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EYE-AM-STUDIOS/aerialsolutions.git"
  },
  "dependencies": {
    "cloudinary": "^1.41.0"
  },
  "devDependencies": {
    "live-server": "^1.2.2"
  }
}